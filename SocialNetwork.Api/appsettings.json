{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AkkaOptions": {
    "ActorSystemName": "SocialNetwork",
    "Hostname": "localhost",
    "Port": 8080,
    "ProtocolType": "TCP",
    "CreationUser": {
      "MaxNumberOfShards": 10,
      "ShardTypename": "Users"
    },
    "Conversation": {
      "MaxNumberOfShards": 10,
      "ShardTypename": "Conversation"
    },
    "Group": {
      "MaxNumberOfShards": 10,
      "ShardTypename": "Group"
    },
    "Channel": {
      "MaxNumberOfShards": 10,
      "ShardTypename": "Channel"
    }
  },
  "AllowedHosts": "*"
}